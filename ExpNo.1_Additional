#include<stdio.h>
#include<math.h>
int main()
{
    float x1,x2,x3,y1,y2,y3,dis1,dis2,dis3;
    scanf("%f%f",&x1,&y1);
    scanf("%f%f",&x2,&y2);
    scanf("%f%f",&x3,&y3);
    
    dis1=sqrt(pow(x2-x1,2)+pow(y2-y1,2));
    dis2=sqrt(pow(x3-x2,2)+pow(y3-y2,2));
    dis3=sqrt(pow(x3-x1,2)+pow(y3-y1,2));
    printf("%.2f\n%.2f\n%.2f",dis1,dis2,dis3);
    
}
